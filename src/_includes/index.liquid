---
layout: default
stylesheets:
  - /css/index.css
---

<main>
  <c-layout>
    {{ content }}
    <h2>Posts</h2>
    <ul>
      {%- for post in collections.post -%}
        <li>
          <a href="{{ post.url }}">{{ post.data.title }}
          </a>
        </li>
      {%- endfor -%}
    </ul>
  </c-layout>
</main>>